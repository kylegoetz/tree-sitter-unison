{
  "name": "tree-sitter-unison",
  "license": "MIT",
  "author": "Kyle Goetz <kylegoetz@gmail.com>",
  "version": "0.0.2",
  "description": "Unison grammar for tree-sitter",
  "repository": "github:kylegoetz/tree-sitter-unison",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "parser",
    "unisonlang",
    "unison"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8",
    "web-tree-sitter": "^0.20.8"
  },
  "scripts": {
    "start": "tree-sitter generate",
    "test": "tree-sitter test",
    "examples": "script/parse-examples",
    "examples-wasm": "script/parse-examples wasm",
    "scratch": "tree-sitter parse test/highlight/scratch.u -d",
    "visual": "tree-sitter parse -D test/highlight/scratch.u"
  },
  "tree-sitter": [
    {
      "scope": "source.unison",
      "file-types": [
        "u"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
